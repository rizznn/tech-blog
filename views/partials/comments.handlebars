<ol class="post-list">
    <li>
    {{#each this}}
      <div class="col-12">
        <div class="card">
          <div class="card-header"></div>
            <div class="comments">
              <section class="comment">
                <div class="comment-text">
                  {{comment_text}}
                </div>
                <div class="comment-meta">
                  --{{user.username}}, {{format_date created_at}}
                </div>
              </section>
            </div>
          </div>
              {{/each}}
      </li>
</ol>
